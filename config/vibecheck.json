{
  "version": 1,
  "tools": {
    "jscpd": {
      "enabled": true,
      "min_tokens": 50
    },
    "dependency_cruiser": {
      "enabled": true,
      "config_path": ".dependency-cruiser.cjs"
    },
    "knip": {
      "enabled": true,
      "config_path": "knip.json"
    }
  },
  "issues": {
    "enabled": true,
    "label": "vibeCheck",
    "max_new_per_run": 20,
    "severity_threshold": "medium",
    "confidence_threshold": "medium"
  }
}
